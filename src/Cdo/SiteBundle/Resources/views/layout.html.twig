<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>
            {% block title %}
                Customer website
            {% endblock title %}
        </title>
        {% block stylesheets %}
            {# Bootstrap #}
            <link rel="stylesheet" href="{{ asset('css/bootstrap.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('css/bootstrap-theme.min.css') }}" />
        {% endblock stylesheets %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <header>
        </header>
        <div id="wrap">
            <div class="container">
                {% block content %}
                    {% block innercontent %}
                    {% endblock innercontent %}
                {% endblock content %}
            </div><!-- /.container -->
        </div><!-- /#wrap -->
        {% block javascripts %}
        {% endblock javascripts %}
    </body>
</html>
